# IFC JSON Chunking Environment Configuration

# Environment
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=DEBUG

# API Configuration
API_HOST=0.0.0.0
API_PORT=8001

# Directories
OUTPUT_DIRECTORY=./output
TEMP_DIRECTORY=./temp
UPLOAD_DIRECTORY=./uploads

# LLM Configuration (Required for query processing)
# Please set your Gemini API key here:
GEMINI_API_KEY=your_gemini_api_key_here
TARGET_LLM_MODEL=gemini-2.0-flash-exp

# Processing Configuration
MAX_CONCURRENT_REQUESTS=5
CHUNK_SIZE_MB=10
MAX_CHUNKS=1000
OVERLAP_PERCENTAGE=0.1

# Cache Configuration (Optional - will fall back to in-memory if not available)
REDIS_URL=redis://localhost:6379/0
CACHE_TTL_SECONDS=3600

# Database Configuration (Optional - will use temporary storage if not available)
DATABASE_URL=sqlite:///./ifc_chunking.db

# Real-world Demo Data
DEMO_DATA_PATH=/Users/max/Downloads/BA Cursor Projekt/IDM_Holzbauteile_mapped_cleaned.json

# Web Interface
FRONTEND_URL=http://localhost:5173
CORS_ORIGINS=["http://localhost:3000", "http://localhost:5173", "http://localhost:8080"]